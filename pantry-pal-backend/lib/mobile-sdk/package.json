{
  "name": "@pantrypal/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for PantryPal API - Kitchen management made easy",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pantry",
    "recipe",
    "meal-planning",
    "kitchen",
    "food",
    "cooking",
    "sdk",
    "api-client",
    "typescript"
  ],
  "author": "PantryPal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/pantry-pal"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pantry-pal/issues"
  },
  "homepage": "https://github.com/yourusername/pantry-pal#readme",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
